<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plug-in>
    <name>cgu</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.0</version>
    <documentation></documentation>
    <installation></installation>
    <changes></changes>
    <user-guide></user-guide>
    <description>cgu.plugin.description</description>
    <provider>cgu.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2018 Mairie de Paris</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>6.1.1</min-core-version>
        <max-core-version/>	
    </core-version-dependency>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>CGU_MANAGEMENT</feature-id>
            <feature-title>cgu.adminFeature.Manage.name</feature-title>
            <feature-description>cgu.adminFeature.Manage.description</feature-description>
            <feature-level>0</feature-level>
            <feature-url>jsp/admin/plugins/cgu/ManageCgus.jsp</feature-url>
            <feature-icon-url></feature-icon-url>
      </admin-feature>
    </admin-features>    

    <!-- XPages parameters -->
    <applications>
        <application>
            <application-id>cgu</application-id>
            <application-class>fr.paris.lutece.plugins.cgu.web.CguXPage</application-class>
        </application>
    </applications>

    <!-- Portlet parameters -->
    <portlets>
   </portlets>
</plug-in>